{
  "name": "school-checker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:clean": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .turbo && npm run dev",
    "dev:fresh": "rm -rf .next && rm -rf node_modules/.cache && rm -rf .turbo && rm -rf .swc && npm run dev",
    "dev:reset": "./scripts/dev-reset.sh",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "clean": "rm -rf .next node_modules/.cache .turbo .swc",
    "data:fetch": "node scripts/data-fetcher.js",
    "data:process": "node scripts/data-processor.js",
    "data:import": "node scripts/data-importer.js",
    "data:pipeline": "npm run data:fetch && npm run data:process && npm run data:import",
    "cron:setup": "node scripts/setup-cron.js",
    "cron:test": "node scripts/test-cron.js",
    "data:auto": "node scripts/automated-data-fetcher.js --now",
    "data:auto:setup": "node scripts/automated-data-fetcher.js --cron",
    "data:auto:install": "bash scripts/setup-automated-fetcher.sh",
    "data:auto:config": "node scripts/setup-custom-csv.js",
    "data:status": "node scripts/data-status.js"
  },
  "dependencies": {
    "@supabase/auth-helpers-react": "^0.5.0",
    "@supabase/supabase-js": "^2.57.0",
    "axios": "^1.6.0",
    "chart.js": "^4.5.0",
    "csv-parser": "^3.2.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.2",
    "fs-extra": "^11.2.0",
    "leaflet": "^1.9.4",
    "next": "15.5.2",
    "node-cron": "^3.0.3",
    "proj4": "^2.19.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.12",
    "typescript": "^5"
  }
}
